{
  "quiet": true,
  "legacyWatch": false,
  "delay": 0.1,
  "watch": [
    "docker/entrypoint.dev.main.sh",
    "poetry.lock",
    "poetry.toml",
    "pyproject.toml",
    "src/**/*.py"
  ],
  "ext": "py,pyi,json,txt,ini,yml,sh,lock",
  "ignore": [
    ".cache",
    ".idea",
    ".pytest_cache",
    ".reports",
    "data",
    "docker",
    "node_modules"
  ],
  "exec": "bash docker/entrypoint.dev.main.sh; true"
}
