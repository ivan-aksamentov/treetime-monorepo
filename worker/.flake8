[flake8]
ignore = E203, E266, E501, W503
exclude = .git,__pycache__,old,build,dist,.cache,.idea,.vscode,.pytest_cache,.reports,config,data,docker,node_modules,.coveragerc,.flake8,.gitignore,.prettierignore,.prettierrc,.pylintrc,.yarnrc,Dockerfile,README.md,mypy.ini,package.json,poetry.lock,poetry.toml,pyproject.toml,pytest.ini,yarn.lock
max-line-length = 80
max-complexity = 18
select = B,C,E,F,W,T4,B9
mypy_config = mypy.ini

